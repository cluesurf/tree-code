{
  "name": "tree-code",
  "icon": "view/tree.png",
  "displayName": "TreeCode",
  "description": "TreeCode Syntax Highlighter",
  "version": "1.1.0",
  "license": "MIT",
  "publisher": "termsurf",
  "main": "./base.js",
  "scripts": {
    "dock": "vsce login termsurf",
    "make": "vsce package",
    "host": "vsce publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/termsurf/tree-code.git"
  },
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "workspaceContains:**/*.tree"
  ],
  "devDependencies": {
    "prettier": "^2.8.1"
  },
  "contributes": {
    "iconThemes": [
      {
        "id": "tree",
        "label": "Tree",
        "path": "./view/view.json"
      }
    ],
    "configurationDefaults": {
      "[tree]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
          "other": true,
          "comments": true,
          "strings": true
        },
        "editor.tabCompletion": "on",
        "editor.wordSeparators": " ()<>{},?/",
        "editor.bracketPairColorization.enabled": false
      }
    },
    "languages": [
      {
        "id": "tree",
        "aliases": [
          "Tree",
          "TreeCode",
          "tree"
        ],
        "extensions": [
          ".tree"
        ],
        "configuration": "./text/bind.json"
      },
      {
        "id": "tree-markdown-injection"
      }
    ],
    "grammars": [
      {
        "language": "tree",
        "scopeName": "source.tree",
        "path": "./text/tree.json"
      },
      {
        "language": "tree-markdown-injection",
        "scopeName": "markdown.tree.codeblock",
        "path": "./text/mark.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.tree": "tree"
        }
      }
    ]
  }
}
